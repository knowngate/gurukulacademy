          "buttons": " <a data-app-btn=\"true\" class=\"btn btn-warning-outline btn-warning\" href=\"iit_colleges.html\" data-toolbar=\"-mbrBtnMove\" target=\"_blank\">IIT Colleges Info</a>"
        },
        {
          "_name": "content1",
          "_styles": {
            "P": {
              "font-family": "'Lora', serif",
              "googleLink": "https://fonts.googleapis.com/css?family=Lora:400,700",
              "font-size": "13px"
            }
          },
          "_anchor": "content1-0",
          "_params": {
            "paddingTop": "0",
            "paddingBottom": "1",
